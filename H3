#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>

#define ROWS 9
#define COLS 9

char seats[ROWS][COLS];

void initSeats() {
    int i, j;
    for (i = 0; i < ROWS; i++) {
        for (j = 0; j < COLS; j++) {
            seats[i][j] = '-';
        }
    }
}

void displaySeats() {
    int i, j;
    printf(" \\123456789\n");
    for (i = ROWS - 1; i >= 0; i--) {
        printf("%d", i + 1);
        for (j = 0; j < COLS; j++) {
            printf("%c", seats[i][j]);
        }
        printf("\n");
    }
}

void autoArrange(int count) {
    int i, j, k;
    for (i = 0; i < ROWS; i++) {
        for (j = 0; j < COLS - count; j++) {
            int ok = 1;
            for (k = 0; k < count; k++) {
                if (seats[i][j + k] != '-') {
                    ok = 0;
                    break;
                }
            }
            if (ok) {
                for (k = 0; k < count; k++) {
                    seats[i][j + k] = '@';
                }
                displaySeats();
                return;
            }
        }
    }
}

int main() {
    initSeats();
    autoArrange(3);
    return 0;
}
