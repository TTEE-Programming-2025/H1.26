#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>

#define ROWS 9
#define COLS 9

char seats[ROWS][COLS];

void initSeats() {
    int i, j;
    for (i = 0; i < ROWS; i++) {
        for (j = 0; j < COLS; j++) {
            seats[i][j] = '-';
        }
    }
}

void displaySeats() {
    int i, j;
    printf(" \\123456789\n");
    for (i = 0; i < ROWS; i++) {
        printf("%d", i + 1);
        for (j = 0; j < COLS; j++) {
            printf("%c", seats[i][j]);
        }
        printf("\n");
    }
}

int login() {
    char input[10];
    int attempts = 0;
    while (attempts < 3) {
        printf("Enter password: ");
        scanf("%s", input);
        if (strcmp(input, "2025") == 0) {
            return 1;
        }
        attempts++;
    }
    return 0;
}

int main() {
    if (login()) {
        initSeats();
        displaySeats();
    }
    return 0;
}
